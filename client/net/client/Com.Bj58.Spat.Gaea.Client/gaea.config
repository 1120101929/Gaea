<?xml version="1.0" encoding="utf-8" ?>
<GAEA>
  <Service name="imc" id="100" maxThreadCount="50">
    <Commmunication >
      <!--
        bufferSize:网络传输流缓存大小
        minPoolSize:连接池中最小连接数量
        maxPoolSize:连接池中最大连接数量
        nagle:是否启用Nagle算法
        autoShrink:是否启动连接自动回收
        sendTimeout:发送超时
        receiveTimeout:接收超时
        waitTimeout:当连接池中没有连接时需要等待释放连接的时间
      -->
      <SocketPool bufferSize="4096" minPoolSize="3" maxPoolSize="100" nagle="true" autoShrink="00:00:20" sendTimeout="00:00:05" receiveTimeout="00:00:10" waitTimeout="00:00:30" />
      <!--
        serialize:序列化方式
        serializeType:扩展序列号类型，程序集与类型名称之间用逗号隔开
        encoder:序列化采用的编码
        compressType:是否压缩，当前版本不支持压缩
      -->
      <Protocol serialize="Gaea" encoder="UTF-8" compressType="UnCompress" />
    </Commmunication>
    <Loadbalance>
      <!--
        deadTimeout:服务器挂后心跳检测间隔时间
        maxCurrentUser:最大当前用户数，超过此数服务器将置为busy状态
      -->
      <Server deadTimeout="00:00:02">
        <add name="imc1" host="127.0.0.1" port="12001" maxCurrentUser="50"  />
        <!--<add name="imc1" host="192.168.9.117" port="16003" maxCurrentUser="50"  />-->
      </Server>
    </Loadbalance>
  </Service>

  <!--
        All,
        Debug,
        Info,
        Notice,
        Warn,
        Error,
        Fatal,
        Off
  -->
  <Logger level="Info" type="Com.Bj58.Spat.Gaea.Client.Utility.Logger.UDPLogger" >
    <UDP host="192.168.100.118" port="8888"/>
    <File path="Gaea_log"/>
  </Logger>
</GAEA>