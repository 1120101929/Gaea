#include "Parameter.h"

namespace gaea {

Parameter::Parameter(int typeId, char *simpleName, void *value, ParaType paraType) {
	this->typeId = typeId;
	this->simpleName = simpleName;
	this->value = value;
	this->paraType = paraType;
}

Parameter::~Parameter() {
}

ParaType Parameter::getParaType() const {
	return paraType;
}

char *Parameter::getSimpleName() const {
	return simpleName;
}

void *Parameter::getValue() const {
	return value;
}

int Parameter::getTypeId() const {
	return typeId;
}

}
